<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="chessmates_dotnet"
                   namespace="chessmates_dotnet.Models"
>
  <class name="RatingStats">
    <id name="Id" type="Int64" generator="native" />
    <property name="Games" />
    <property name="Rating" />
    <property name="Prog" column="Progression" />
    <property name="Rd" column="Deviation" />
    <property name="GameType" />
    <many-to-one name="Player" column="PlayerId" class="Player" />
  </class>
</hibernate-mapping>